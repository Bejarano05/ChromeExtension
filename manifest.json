{
    "manifest_version": 2,
    "name": "Contador de cadena",
    "version": "0.1",
    "description": "My first chrome extension",

    "background": {
        "persistent": true,
        "scripts":["js/background.js"]
    },

    "permissions": [
        "contextMenus",
        "activeTab"
    ],

    "browser_action": {
        "default_icon": "icon.png"
    }
}